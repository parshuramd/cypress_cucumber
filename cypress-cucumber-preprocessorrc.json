{
  "json": {
    "enabled": true,
    "formatter": ".bin/cucumber-json-formatter",
    "output": "cypress/reports/json/cucumber-report.json"
  },
    "messages": {
      "enabled": false,
      "output": "jsonlogs/messages.ndjson"
    },
    "html": {
      "enabled": true
    },
    "stepDefinitions": [
      "[filepath]/**/*.{js,ts}",
      "[filepath].{js,ts}",
      "cypress/e2e/*.{js,ts}",
      "[filepath]\\***.{js,ts}",
      "[filepath].{js,ts}",
      "cypress\\e2e\\*.{js,ts}"
    ]
  }